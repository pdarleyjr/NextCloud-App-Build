{
  "name": "appointments",
  "version": "2.0.0",
  "license": "agpl",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "webpack --config webpack.dev.js",
    "watch": "webpack --progress --watch --config webpack.dev.js",
    "build": "NODE_OPTIONS=--max-old-space-size=4096 webpack --progress --config webpack.prod.js",
    "build:analyze": "NODE_OPTIONS=--max-old-space-size=4096 WEBPACK_ANALYZE=true webpack --progress --config webpack.prod.js",
    "datepicker-css": "sass --style=compressed --no-source-map scss/datepicker/index.scss css/datepicker.css",
    "convert-icons": "node nc25icons.js",
    "profile": "NODE_OPTIONS=--max-old-space-size=4096 webpack --config webpack.prod.js --profile --json > stats.json",
    "lint": "eslint --ext .js,.vue src",
    "verify-scss": "./verify-scss-files.sh"
  },
  "dependencies": {
    "@nextcloud/axios": "^2.5.1",
    "@nextcloud/dialogs": "^5.3.8",
    "@nextcloud/l10n": "^3.1.0",
    "@nextcloud/logger": "^3.0.2",
    "@nextcloud/router": "^3.0.1",
    "@nextcloud/vue": "^8.20.0",
    "linkifyjs": "^4.1.3",
    "pinia": "^2.2.6",
    "vue": "^2.7.16",
    "vue-material-design-icons": "^5.3.1",
    "vue-slider-component": "^3.2.24",
    "vue2-datepicker": "^3.11.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/plugin-transform-shorthand-properties": "^7.23.3",
    "@nextcloud/babel-config": "^1.0.0",
    "@nextcloud/browserslist-config": "^3.0.0",
    "@vue/compiler-sfc": "^3.3.11",
    "@vue/test-utils": "^1.2.0",
    "assert": "^2.0.0",
    "autoprefixer": "^10.4.21",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.9.0",
    "file-loader": "^6.2.0",
    "postcss": "^8.5.3",
    "process": "^0.11.10",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "terser-webpack-plugin": "^5.3.9",
    "util": "^0.12.5",
    "uuid": "^9.0.0",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.10.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ]
}
